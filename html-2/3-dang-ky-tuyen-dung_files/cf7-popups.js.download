			html: cf7_popups_val.msg7,
			showCloseButton: true,
			showConfirmButton: false,
		});
	} else {
		swal({
			title: '<strong>'+cf7_popups_val.msg113+'</strong>',
			type: 'success',
			html: cf7_popups_val.msg114,
			showCloseButton: true,
			showConfirmButton: false,
		});
	}
	jQuery('.popmake-close').click();
}, false );



